{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/css/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|html))",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ]
}